{
  "name": "solophp/container",
  "type": "library",
  "description": "PSR-11 compatible Dependency Injection Container",
  "keywords": ["PHP","DI","Container","PSR-11"],
  "license": "MIT",
  "authors": [
    {
      "name": "Vitaliy Olos",
      "email": "vedastudio@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Solo\\Container\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Solo\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "psr/container": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "scripts": {
    "test": "phpunit",
    "cs": "phpcs --standard=PSR12 src/ tests/",
    "cs-fix": "phpcbf --standard=PSR12 src/ tests/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
